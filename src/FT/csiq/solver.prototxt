# The train/test net protocol buffer definition
net: "src/FT/csiq/train.prototxt"

test_iter: 1

test_interval: 40
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 2000
display: 20
max_iter: 8000
momentum: 0.9
solver_type: SGD
weight_decay: 0.0005
average_loss: 500
snapshot: 8000
snapshot_prefix: "models/csiq/FT"
solver_mode: GPU
