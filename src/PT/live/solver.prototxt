# The train/test net protocol buffer definition
net: "src/PT/live/train.prototxt"

test_iter: 1

test_interval: 100
base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 8000
display: 20
max_iter: 24000
momentum: 0.9
solver_type: SGD
weight_decay: 0.0005
average_loss: 500
snapshot: 24000
snapshot_prefix: "models/live/PT"
solver_mode: GPU
