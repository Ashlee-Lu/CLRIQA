# The train/test net protocol buffer definition
net: "src/PT/csiq/train.prototxt"

test_iter: 1

test_interval: 200
base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 6000
display: 30
max_iter: 18000
momentum: 0.9
solver_type: SGD
weight_decay: 0.0005
average_loss: 500
snapshot: 18000
snapshot_prefix: "models/csiq/PT"
solver_mode: GPU
